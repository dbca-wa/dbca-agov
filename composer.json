{
    "name": "dbca-wa/drupal-agov",
    "description": "Project template for Drupal 8 agov projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.5",
        "cweagans/composer-patches": "^1.6",
        "drupal-composer/drupal-scaffold": "^2.2",
        "drupal/admin_toolbar": "^1.23",
        "drupal/agov": "^2.0",
        "drupal/agov_base": "^1.2",
        "drupal/agov_whitlam": "^1.3",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/ds": "^3.1",
        "drupal/fences": "^2.0@alpha",
        "drupal/link_attributes": "^1.2",
        "drupal/linkit": "^4.3",
        "drupal/linky": "^1.0@alpha",
        "drupal/media_entity_image": "^1.2",
        "drupal/page_manager": "^4.0@beta",
        "drupal/pathauto": "^1.1",
        "drupal/pnx_media_embed": "^1.0@alpha",
        "drupal/token": "^1.1",
        "drupal/twitter_block": "^2.1",
        "hirak/prestissimo": "^0.3.7",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "webflo/drupal-core-require-dev": "~8.5.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "patch -d web/modules/contrib/pnx_media/config/install/ -i patches/pnx_media/2937899-2.patch",
            "cp -Tav src web"
        ],
        "post-update-cmd": [
            "@drupal-scaffold",
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "cp -Tav src web"
        ]
    },
    "extra": {
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
}
